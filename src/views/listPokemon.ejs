 <%- include('partials/header') %>

    <div class="container">
        <% for(const pokemon of allPokemon) {%>
        <article class="card">
            <a class="card__link"href="/detail/<%= pokemon.numero %> ">
                <img class="card__img"src="/img/<%= pokemon.numero %>.png" alt="illustration de <%= pokemon.nom %>">
                <h3 class="card__title">#<%= pokemon.numero %> <%= pokemon.nom %></h3>
            </a>
        </article>
        <% }%> 
    </div>
    
</body>
</html>